<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="OSDevV2" />
		<Option pch_mode="2" />
		<Option compiler="llvm_clang_compiler_osdev" />
		<Build>
			<Target title="Boot">
				<Option output="bin/Boot/boot.bin" prefix_auto="1" extension_auto="0" />
				<Option object_output="obj/Boot/" />
				<Option type="1" />
				<Option compiler="llvm_clang_compiler_osdev" />
				<Compiler>
					<Add option="-Os" />
					<Add option="-m16 -masm=intel" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-T boot.ld" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="--target=i686-pc-none -march=i686 -ffreestanding -fno-builtin -nostdlib -nostdinc -nostdinc++" />
		</Compiler>
		<Linker>
			<Add option="--oformat binary" />
		</Linker>
		<Unit filename="src/boot/stage1.asm">
			<Option compiler="llvm_clang_compiler_osdev" use="1" buildCommand="nasm $file -f elf -o $object" />
		</Unit>
		<Unit filename="src/boot/stage2.asm">
			<Option compile="0" />
			<Option link="0" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
