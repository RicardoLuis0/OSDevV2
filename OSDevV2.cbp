<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="OSDevV2" />
		<Option pch_mode="2" />
		<Option compiler="llvm_clang_compiler_osdev" />
		<Build>
			<Target title="Kernel">
				<Option output="bin/kernel.bin" prefix_auto="1" extension_auto="0" />
				<Option object_output="obj/" />
				<Option type="1" />
				<Option compiler="llvm_clang_compiler_osdev" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add directory="include" />
				</Compiler>
				<Linker>
					<Add option="-T linker.ld" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="--target=i686-pc-none-elf -march=i686 -ffreestanding -fno-builtin -nostdlib -nostdinc -nostdinc++ -fno-rtti -fno-exceptions" />
		</Compiler>
		<Unit filename="src/boot.asm">
			<Option compiler="llvm_clang_compiler_osdev" use="1" buildCommand="nasm $file -f elf -o $object" />
		</Unit>
		<Unit filename="src/kmain.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
